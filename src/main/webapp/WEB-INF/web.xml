<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>ServletPrueba</servlet-name>
		<display-name>ServletPrueba</display-name>
		<description></description>
		<servlet-class>es.open4job.proyecto.web.ServletPrueba</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServletPruebaSession</servlet-name>
		<display-name>ServletPruebaSession</display-name>
		<description></description>
		<servlet-class>es.open4job.proyecto.web.ServletPruebaSession</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>svPruebaSessionId</servlet-name>
		<display-name>svPruebaSessionId</display-name>
		<description></description>
		<servlet-class>es.open4job.proyecto.web.svPruebaSessionId</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletPrueba</servlet-name>
		<url-pattern>/ServletPrueba</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletPruebaSession</servlet-name>
		<url-pattern>/ServletPruebaSession</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>svPruebaSessionId</servlet-name>
		<url-pattern>/svPruebaSessionId</url-pattern>
	</servlet-mapping>
	<resource-ref>
		<description>Oracle Datasource example</description>
		<res-ref-name>jdbc/myoracleprueba</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
</web-app>
